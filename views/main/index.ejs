<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <%- include ('../partials/head.ejs') %>
</head>

<body>
  <div id="container">
    <%- include ('../partials/nav.ejs') %>
    <div id="firstContainer" class="container-fluid border" style="max-width: 1100px;">
      <div class="row">
        Hello
      </div>
      <!-- Row -->
      <div class="row" style="display: flex; justify-content: center;">
        <div class="col-sm-8">
          <%for (let i = 0; i < data.length; i++) { %>
          <!-- Card -->
          <div class="card mb-4 border-info" style="max-width: 340px;">
            <div class="row">
              <div class="col-md-4">
                <img src="..." alt="Image">
              </div>
              <div class="col-md-8">
                <div class="card-body">
                  <h5 class="card-title"><%=data[i].name%></h5>
                  <p class="card-text"> Age: <%=data[i].age %></p>
                  <p class="card-text">Breed: <%=data[i].breed%></p>
                </div>
              </div>
            </div>
          </div>
          <!-- End of card -->
          <% } %>
        </div>
      </div>
    </div>

  </div>
  </div>
</body>

</html>